				<svg
					xmlns="http://www.w3.org/2000/svg"
					class="w-4 h-4 mr-2 text-gray-800 dark:text-gray-200"
					fill="none"
					viewBox="0 0 24 24"
					stroke="currentColor"
					focusable="false"
				>
					<path
						stroke-linecap="round"
						stroke-linejoin="round"
						stroke-width="2"
						d="M20.354 15.354A9 9 0 018.646 3.646 9.003 9.003 0 0012 21a9.003 9.003 0 008.354-5.646z"
					/>
				</svg>
				Dark
				<svg
					x-show="$store.theme.mode === 'dark'"
					class="w-4 h-4 ml-auto text-blue-500"
					fill="none"
					stroke="currentColor"
					viewBox="0 0 24 24"
					focusable="false"
				>
					<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
				</svg>
			</button>
			<!-- System Option -->
			<button
				@click="$store.theme.mode = 'system'; $store.theme.updateTheme(); open = false"
				class="flex items-center w-full px-3 py-2 transition-colors rounded hover:bg-gray-100 dark:hover:bg-zinc-800"
				:class="$store.theme.mode === 'system' ? 'font-semibold' : ''"
				type="button"
			>
				<svg
					xmlns="http://www.w3.org/2000/svg"
					class="w-4 h-4 mr-2 text-gray-500"
					fill="none"
					viewBox="0 0 24 24"
					stroke="currentColor"
					focusable="false"
				>
					<path
						stroke-linecap="round"
						stroke-linejoin="round"
						stroke-width="2"
						d="M9.75 17L9 20l-1 1h8l-1-1-.75-3M3 13h18M5 17h14a2 2 0 002-2V5a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z"
					/>
				</svg>
				System
				<svg
					x-show="$store.theme.mode === 'system'"
					class="w-4 h-4 ml-auto text-blue-500"
					fill="none"
					stroke="currentColor"
					viewBox="0 0 24 24"
					focusable="false"
				>
					<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
				</svg>
			</button>
		</div>
	</div>
</html>
